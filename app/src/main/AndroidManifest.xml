<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.fitfeed">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".FitFeed"
        android:allowBackup="true"
        android:icon="@drawable/launchicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.hp.fitfeed.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAI0N" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/register" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ViewProfileActivity"></activity>
        <activity android:name=".FragmentActivity"></activity>
    </application>

</manifest>